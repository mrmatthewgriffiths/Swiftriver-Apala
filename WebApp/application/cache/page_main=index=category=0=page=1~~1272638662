	<script type="text/javascript">
$(function() {
		$("#slider-range").slider({
			range: true,
			min: 0,
			max: 100,
				values: [0 , 100 ],
			slide: function(event, ui) {
				$("#veracity_min").val(ui.values[0]);
			  $("#veracity_max").val(ui.values[1]);
			}
		});
		$("#veracity_min").val( $("#slider-range").slider("values", 0));
		$("#veracity_max").val( $("#slider-range").slider("values", 1));
	});

	</script>
